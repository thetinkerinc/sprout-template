{
	"name": "sprout-app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"dev": "run-s dev:db dev:web",
		"dev:db": "docker compose up -d",
		"dev:web": "vite dev",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"cleanup": "run-s format lint check",
		"format": "prettier --write . '!data/**'",
		"lint": "eslint .",
		"prepare": "svelte-kit sync",
		"preview": "vite preview",
		"studio": "dotenvx run -- drizzle-kit studio",
		"cf-typegen": "wrangler types src/worker-configuration.d.ts"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "^1.51.4",
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.39.2",
		"@internationalized/date": "^3.10.1",
		"@lucide/svelte": "^0.562.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.11",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"bits-ui": "^2.15.2",
		"clsx": "^2.1.1",
		"drizzle-kit": "^1.0.0-beta.8-dbc3565",
		"drizzle-orm": "^1.0.0-beta.8-dbc3565",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^17.0.0",
		"kysely-ctl": "^0.19.0",
		"mode-watcher": "^1.1.0",
		"npm-run-all": "^4.1.5",
		"pg": "^8.16.3",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.46.1",
		"svelte-check": "^4.3.5",
		"svelte-sonner": "^1.0.7",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.18",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.51.0",
		"vite": "^7.3.0",
		"wrangler": "^4.54.0"
	},
	"dependencies": {
		"@clerk/localizations": "^3.32.1",
		"@inlang/paraglide-js": "^2.7.1",
		"@thetinkerinc/commander": "^0.1.4",
		"@thetinkerinc/isolocal": "^0.1.1",
		"@thetinkerinc/sprout": "^0.1.5",
		"date-fns": "^4.1.0",
		"kysely": "^0.28.9",
		"svelte-clerk": "^0.20.3"
	}
}
